  "cds": {
    "features": {
      "graphql": true
    },
    "requires": {
      "[development]": {
        "sapbackend": {
          "kind": "odata-v2",
          "model": "srv/external/sapbackend",
          "credentials": {
            "url": "http://erp13.sap4practice.com:9037/sap/opu/odata/sap/YSAPUI5_SRV_01/"
          }
        }
      }
    },
      "i18n": {
      "default_language": "es",
      "folders": [
        "_i18n",
        "i18n",
        "assets/i18n"
      ]
    }
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ]
  },
  "workspaces": [
